:plain
  $('#ext_dialog').wijdialog({
    width: 840, 
    height: 640, 
    contentUrl: '#{contacts_path}', 
    autoOpen: true,
    title: 'Contacts',
    dialogClass: "dialogWithDropShadow",
    show: 'puff',
    hide: 'explode',
    collapsingAnimation: 
      { animated: "puff", duration: 300, easing: "easeOutExpo" },
    expandingAnimation: 
      { animated: "highlight", duration: 300, easing: "easeOutExpo" },
    modal: true,
  });
